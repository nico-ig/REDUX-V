v0x559bb8144bb0_0 .net "write_enable", 0 0, v0x559bb8145180_0;  1 drivers
E_0x559bb8123ba0 .event posedge, v0x559bb8144810_0;
E_0x559bb810e9b0 .event negedge, v0x559bb8144810_0;
    .scope S_0x559bb8126f20;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x559bb81449c0_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_0x559bb8126f20;
T_1 ;
    %wait E_0x559bb810e9b0;
    %load/vec4 v0x559bb8144bb0_0;
    %cmpi/e 0, 0, 1;
    %flag_mov 8, 6;
    %jmp/0 T_1.0, 8;
    %load/vec4 v0x559bb8114d50_0;
    %pad/u 10;
    %ix/vec4 4;
    %load/vec4a v0x559bb8144aa0, 4;
    %xor/r;
    %cmpi/e 1, 1, 1;
    %flag_mov 9, 6;
    %jmp/0 T_1.2, 9;
    %pushi/vec4 0, 0, 8;
    %jmp/1 T_1.3, 9;
T_1.2 ; End of true expr.
    %load/vec4 v0x559bb8114d50_0;
    %pad/u 10;
    %ix/vec4 4;
    %load/vec4a v0x559bb8144aa0, 4;
    %jmp/0 T_1.3, 9;
 ; End of false expr.
    %blend;
T_1.3;
    %jmp/1 T_1.1, 8;
T_1.0 ; End of true expr.
    %load/vec4 v0x559bb81449c0_0;
    %jmp/0 T_1.1, 8;
 ; End of false expr.
    %blend;
T_1.1;
    %store/vec4 v0x559bb81449c0_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_0x559bb8126f20;
T_2 ;
    %wait E_0x559bb8123ba0;
    %load/vec4 v0x559bb8144bb0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_2.0, 6;
    %load/vec4 v0x559bb81448d0_0;
    %load/vec4 v0x559bb8114d50_0;
    %pad/u 10;
    %ix/vec4 4;
    %store/vec4a v0x559bb8144aa0, 4, 0;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x559bb8126d90;
T_3 ;
