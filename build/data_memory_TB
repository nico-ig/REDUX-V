    %load/vec4 v0x559bb8144df0_0;
    %nor/r;
    %store/vec4 v0x559bb8144df0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x559bb8126d90;
T_5 ;
    %vpi_call 2 31 "$monitor", "%10d    0x%02H    0x%02H    0x%02H    0x%02H    0x%02H    0x%02H", $time, v0x559bb8144df0_0, v0x559bb8145180_0, v0x559bb8144d10_0, v0x559bb8144ec0_0, v0x559bb8144fc0_0, v0x559bb8145090_0 {0 0 0};
