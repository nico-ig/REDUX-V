    %load/vec4 v0x559bb8144df0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_6.0, 4;
    %load/vec4 v0x559bb8145180_0;
    %nor/r;
    %store/vec4 v0x559bb8145180_0, 0, 1;
    %load/vec4 v0x559bb8145180_0;
