#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 15;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5620d4c4bb70 .scope module, "pc_TB" "pc_TB" 2 3;
 .timescale -15 -15;
v0x5620d4c64170_0 .var "clk", 0 0;
v0x5620d4c64240_0 .var "expected", 7 0;
v0x5620d4c64300_0 .var "next_pc", 7 0;
v0x5620d4c64400_0 .net "pc", 7 0, v0x5620d4c64000_0;  1 drivers
E_0x5620d4c4ab00 .event edge, v0x5620d4c39cb0_0;
S_0x5620d4c4bd00 .scope module, "DUT" "pc" 2 16, 3 1 0, S_0x5620d4c4bb70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 8 "next_pc";
    .port_info 2 /OUTPUT 8 "pc";
P_0x5620d4c4bee0 .param/l "BITS" 0 3 2, +C4<00000000000000000000000000001000>;
v0x5620d4c39cb0_0 .net "clk", 0 0, v0x5620d4c64170_0;  1 drivers
v0x5620d4c63f20_0 .net "next_pc", 7 0, v0x5620d4c64300_0;  1 drivers
v0x5620d4c64000_0 .var "pc", 7 0;
E_0x5620d4c4b030 .event negedge, v0x5620d4c39cb0_0;
    .scope S_0x5620d4c4bd00;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5620d4c64000_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_0x5620d4c4bd00;
T_1 ;
    %wait E_0x5620d4c4b030;
    %load/vec4 v0x5620d4c63f20_0;
    %assign/vec4 v0x5620d4c64000_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5620d4c4bb70;
T_2 ;
    %vpi_call 2 10 "$dumpfile", "pc.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5620d4c4bb70 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5620d4c4bb70;
T_3 ;
    %delay 4, 0;
    %load/vec4 v0x5620d4c64170_0;
    %nor/r;
    %store/vec4 v0x5620d4c64170_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5620d4c4bb70;
T_4 ;
    %vpi_call 2 25 "$monitor", "%10d    0x%02H    0x%02H    0x%02H    0x%02H", $time, v0x5620d4c64170_0, v0x5620d4c64400_0, v0x5620d4c64300_0, v0x5620d4c64240_0 {0 0 0};
    %vpi_call 2 27 "$display", "\012### Test: pc" {0 0 0};
    %vpi_call 2 28 "$display", "--------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 29 "$display", "%10s %7s %6s   %7s %8s", "TIME", "CLK", "PC", "NEXT_PC", "EXPECTED" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5620d4c64170_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5620d4c64300_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5620d4c64240_0, 0, 8;
    %end;
    .thread T_4;
    .scope S_0x5620d4c4bb70;
T_5 ;
    %wait E_0x5620d4c4ab00;
    %load/vec4 v0x5620d4c64170_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_5.0, 4;
    %load/vec4 v0x5620d4c64300_0;
    %store/vec4 v0x5620d4c64240_0, 0, 8;
    %delay 1, 0;
    %load/vec4 v0x5620d4c64400_0;
    %load/vec4 v0x5620d4c64240_0;
    %cmp/ne;
    %jmp/0xz  T_5.2, 6;
    %vpi_call 2 41 "$fatal", 32'sb00000000000000000000000000000000, "ASSERTION FAILED -- expected: 0x%02H, actual: 0x%02H", v0x5620d4c64240_0, v0x5620d4c64400_0 {0 0 0};
T_5.2 ;
    %load/vec4 v0x5620d4c64240_0;
    %cmpi/e 255, 0, 8;
    %jmp/0xz  T_5.4, 4;
    %vpi_call 2 40 "$display" {0 0 0};
    %vpi_call 2 41 "$finish" {0 0 0};
T_5.4 ;
    %jmp T_5.1;
T_5.0 ;
    %load/vec4 v0x5620d4c64240_0;
    %addi 1, 0, 8;
    %store/vec4 v0x5620d4c64300_0, 0, 8;
T_5.1 ;
    %jmp T_5;
    .thread T_5, $push;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "../pc/pc_TB.v";
    "../pc/pc.v";
