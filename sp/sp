S_0x562410d7d080 .scope module, "sp_TB" "sp_TB" 2 3;
 .timescale -15 -15;
v0x562410d9a670_0 .var "clk", 0 0;
v0x562410d9a730_0 .var "expected", 7 0;
v0x562410d9a7f0_0 .var "op", 0 0;
v0x562410d9a8f0_0 .net "sp", 7 0, v0x562410d9a400_0;  1 drivers
v0x562410d9a9c0_0 .var "write_enable", 0 0;
E_0x562410d7af90 .event edge, v0x562410d6b200_0;
S_0x562410d7d210 .scope module, "DUT" "sp" 2 17, 3 1 0, S_0x562410d7d080;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "op";
    .port_info 2 /INPUT 1 "write_enable";
    .port_info 3 /OUTPUT 8 "sp";
P_0x562410d7d3f0 .param/l "BITS" 0 3 2, +C4<00000000000000000000000000001000>;
v0x562410d6b200_0 .net "clk", 0 0, v0x562410d9a670_0;  1 drivers
v0x562410d9a250_0 .net "op", 0 0, v0x562410d9a7f0_0;  1 drivers
v0x562410d9a310_0 .var "reg_sp", 7 0;
v0x562410d9a400_0 .var "sp", 7 0;
v0x562410d9a4e0_0 .net "write_enable", 0 0, v0x562410d9a9c0_0;  1 drivers
E_0x562410d42880 .event posedge, v0x562410d6b200_0;
    .scope S_0x562410d7d210;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x562410d9a310_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_0x562410d7d210;
T_1 ;
    %wait E_0x562410d42880;
    %load/vec4 v0x562410d9a4e0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x562410d9a250_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x562410d9a310_0;
    %pad/u 32;
    %cmpi/u 255, 0, 32;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x562410d9a310_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x562410d9a310_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x562410d9a250_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %pushi/vec4 0, 0, 32;
    %load/vec4 v0x562410d9a310_0;
    %pad/u 32;
    %cmp/u;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %load/vec4 v0x562410d9a310_0;
    %subi 1, 0, 8;
    %assign/vec4 v0x562410d9a310_0, 0;
T_1.4 ;
T_1.3 ;
T_1.0 ;
    %load/vec4 v0x562410d9a310_0;
    %assign/vec4 v0x562410d9a400_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x562410d7d080;
T_2 ;
    %vpi_call 2 11 "$dumpfile", "sp.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x562410d7d080 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x562410d7d080;
T_3 ;
    %delay 4, 0;
    %load/vec4 v0x562410d9a670_0;
    %nor/r;
    %store/vec4 v0x562410d9a670_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x562410d7d080;
T_4 ;
    %vpi_call 2 27 "$monitor", "%10d    0x%02H    0x%02H    0x%02H    0x%02H    0x%02H", $time, v0x562410d9a670_0, v0x562410d9a9c0_0, v0x562410d9a7f0_0, v0x562410d9a8f0_0, v0x562410d9a730_0 {0 0 0};
    %vpi_call 2 29 "$display", "\012### Test: stack pointer" {0 0 0};
    %vpi_call 2 30 "$display", "-------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 31 "$display", "%10s %7s %6s %7s %7s   %8s", "TIME", "CLK", "WE", "OP", "SP", "EXPECTED" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562410d9a670_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562410d9a9c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562410d9a7f0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x562410d9a730_0, 0, 8;
    %end;
    .thread T_4;
    .scope S_0x562410d7d080;
T_5 ;
    %wait E_0x562410d7af90;
    %load/vec4 v0x562410d9a670_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_5.0, 4;
    %load/vec4 v0x562410d9a9c0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_5.2, 4;
    %load/vec4 v0x562410d9a7f0_0;
    %cmpi/e 1, 0, 1;
    %flag_mov 8, 4;
    %jmp/0 T_5.4, 8;
    %load/vec4 v0x562410d9a730_0;
    %addi 1, 0, 8;
    %jmp/1 T_5.5, 8;
T_5.4 ; End of true expr.
    %load/vec4 v0x562410d9a730_0;
    %subi 1, 0, 8;
    %jmp/0 T_5.5, 8;
 ; End of false expr.
    %blend;
T_5.5;
    %store/vec4 v0x562410d9a730_0, 0, 8;
    %load/vec4 v0x562410d9a730_0;
    %pushi/vec4 0, 0, 8;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x562410d9a7f0_0;
    %pushi/vec4 1, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.6, 8;
    %load/vec4 v0x562410d9a730_0;
    %subi 1, 0, 8;
    %store/vec4 v0x562410d9a730_0, 0, 8;
    %load/vec4 v0x562410d9a7f0_0;
    %nor/r;
    %store/vec4 v0x562410d9a7f0_0, 0, 1;
    %vpi_call 2 46 "$display" {0 0 0};
    %vpi_call 2 47 "$display", "%10s %7s %6s %7s %7s   %8s", "TIME", "CLK", "WE", "OP", "SP", "EXPECTED" {0 0 0};
    %jmp T_5.7;
T_5.6 ;
    %load/vec4 v0x562410d9a730_0;
    %pushi/vec4 255, 0, 8;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x562410d9a7f0_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.8, 8;
    %vpi_call 2 49 "$display" {0 0 0};
    %vpi_call 2 50 "$finish" {0 0 0};
T_5.8 ;
T_5.7 ;
T_5.2 ;
    %load/vec4 v0x562410d9a9c0_0;
    %nor/r;
    %store/vec4 v0x562410d9a9c0_0, 0, 1;
    %jmp T_5.1;
T_5.0 ;
    %delay 1, 0;
    %load/vec4 v0x562410d9a8f0_0;
    %load/vec4 v0x562410d9a730_0;
    %cmp/ne;
    %jmp/0xz  T_5.10, 6;
    %vpi_call 2 58 "$fatal", 32'sb00000000000000000000000000000000, "ASSERTION FAILED -- expected: 0x%02H, actual: 0x%02H", v0x562410d9a730_0, v0x562410d9a8f0_0 {0 0 0};
T_5.10 ;
T_5.1 ;
    %jmp T_5;
    .thread T_5, $push;
# The file index is used to find the file name in the following table.
