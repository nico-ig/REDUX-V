S_0x5559afde17b0 .scope module, "data_memory_TB" "data_memory_TB" 2 3;
 .timescale -15 -15;
v0x5559afdfb2b0_0 .var "address", 7 0;
v0x5559afdfb390_0 .var "clk", 0 0;
