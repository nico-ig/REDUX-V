    .scope S_0x5559afde17b0;
T_5 ;
    %wait E_0x5559afde3df0;
    %load/vec4 v0x5559afdfb390_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_5.0, 4;
    %load/vec4 v0x5559afdfb720_0;
    %nor/r;
    %store/vec4 v0x5559afdfb720_0, 0, 1;
    %load/vec4 v0x5559afdfb720_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_5.2, 4;
    %load/vec4 v0x5559afdfb2b0_0;
    %addi 1, 0, 8;
    %store/vec4 v0x5559afdfb2b0_0, 0, 8;
    %load/vec4 v0x5559afdfb2b0_0;
    %cmpi/e 0, 0, 8;
    %jmp/0xz  T_5.4, 4;
