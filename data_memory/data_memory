    %jmp T_5.3;
T_5.2 ;
    %load/vec4 v0x5559afdfb460_0;
    %store/vec4 v0x5559afdfb630_0, 0, 8;
    %delay 1, 0;
    %load/vec4 v0x5559afdfb560_0;
    %load/vec4 v0x5559afdfb630_0;
